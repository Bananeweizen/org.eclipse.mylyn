    <hudson.tasks.Shell>
      <command>git rev-parse HEAD >  org.eclipse.mylyn.context.stamp

curl --cacert /mylyn-setup/mylyn_ssl/#CERT_FILE_NAME#.cert -O https://#MYLYN_TEST_NAME#/jenkinsci/job/mylyn-commons-nightly/lastSuccessfulBuild/artifact/org.eclipse.mylyn.commons.stamp
curl --cacert /mylyn-setup/mylyn_ssl/#CERT_FILE_NAME#.cert -O https://#MYLYN_TEST_NAME#/jenkinscin/job/mylyn-versions-nightly/lastSuccessfulBuild/artifact/org.eclipse.mylyn.versions.stamp
curl --cacert /mylyn-setup/mylyn_ssl/#CERT_FILE_NAME#.cert -O https://#MYLYN_TEST_NAME#/jenkinsci/job/mylyn-tasks-nightly/lastSuccessfulBuild/artifact/org.eclipse.mylyn.versions.stamp
rm -rf /downloads/mylyn/snapshots/nightly/$BRANCH/context/*
cp -r org.eclipse.mylyn.context.stamp org.eclipse.mylyn.context-site/target/site/* /downloads/mylyn/snapshots/nightly/$BRANCH/context/
