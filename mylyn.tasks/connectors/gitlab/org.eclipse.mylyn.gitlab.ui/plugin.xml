 <plugin>
   <extension
         id="org.eclipse.mylyn.gitlab.repository"
         name="Gitlab Respository"
         point="org.eclipse.mylyn.tasks.ui.repositories">
      <connectorUi
            brandingIcon="icons/eview16/gitlab-icon.gif"
            class="org.eclipse.mylyn.gitlab.ui.GitlabConnectorUI"
            overlayIcon="icons/eview16/gitlab-overlay.gif">
      </connectorUi>
   </extension>

   <extension
         point="org.eclipse.mylyn.tasks.ui.editors">
      <pageFactory
            class="org.eclipse.mylyn.gitlab.ui.GitlabTaskEditorPageFactory"
            id="org.eclipse.mylyn.gitlab.ui.pageFactory">
      </pageFactory>
   </extension>
</plugin>
